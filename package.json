{
  "name": "quotetyper",
  "version": "1.0.0",
  "description": "Type quotes fast",
  "main": "server.js",
  "scripts": {
    "build": "npm run clean && babel -d ./build ./src -s",
    "start": "npm run build && nodemon ./src/server.js | bunyan",
    "clean": "rm -rf ./build && mkdir build",
    "heroku-postbuild": "npm install && npm run build"
  },
  "author": "Phaedrus and Nathan",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "body-parser": "^1.18.3",
    "bunyan": "^1.8.12",
    "connect-mongo": "^2.0.3",
    "cookie-parser": "^1.4.3",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "jsonwebtoken": "^8.4.0",
    "module-alias": "^2.1.0",
    "mongoose": "^5.4.6",
    "nodemon": "^1.18.9",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0"
  },
  "_moduleAliases": {
    "@root": "src",
    "@routes": "src/routes",
    "@models": "src/models"
  }
}
